<rpc xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" message-id="{message_id}">
  <edit-config>
    <target>
      <candidate/>
    </target>
    <config>
      <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
        <interface>
          <GigabitEthernet>
            <name>{interface_physique}</name>  <!-- ex: 4 -->
            <subinterface>
              <id>{subinterface_id}</id>       <!-- ex: 6 pour 4.6 -->
              <encapsulation>
                <dot1Q>
                  <vlan-id>{vlan_id}</vlan-id>
                </dot1Q>
              </encapsulation>
              <ip>
                <address>
                  <primary>
                    <address>{ip}</address>
                    <mask>{mask}</mask>
                  </primary>
                </address>
              </ip>
            </subinterface>
          </GigabitEthernet>
        </interface>
      </native>
    </config>
  </edit-config>
</rpc>
